    "def find_area(cube):\n",
    "    min_lat = cube.extract(iris.Constraint(latitude=upperlat_bound))\n",
    "    max_lat = min_lat.extract(iris.Constraint(latitude=lowerlat_bound))\n",
    "    min_lon = max_lat.extract(iris.Constraint(longitude=upperlon_bound))\n",
    "    max_lon = min_lon.extract(iris.Constraint(longitude=lowerlon_bound))\n",
    "    area_of_interest = max_lon\n",
    "    return area_of_interest\n",
    "\n",
    "def create_sos_profile(cube):\n",
    "    #Takes the mean of the speed of sound across an area\n",
    "    mean_lon = cube.collapsed('longitude', iris.analysis.MEAN)\n",
    "    full_mean = mean_lon.collapsed('latitude', iris.analysis.MEAN)\n",
    "    return full_mean\n",
    "\n",
    "def create_2dsos_profile(cube):\n",
    "    #The future cubes are still in the 2D lat and 2D lon coords so will now need to be regridded\n",
    "    #Takes the mean of the speed of sound across an area\n",
    "    mean_lon = cube.collapsed('projection_y_coordinate', iris.analysis.MEAN)\n",
    "    full_mean = mean_lon.collapsed('projection_x_coordinate', iris.analysis.MEAN)\n",
    "    return full_mean\n",
    "\n",
    "def make_profile(file_list):\n",
    "    sos_list = []\n",
    "    if file_list == summer_future or file_list == winter_future:\n",
    "        for file in file_list:\n",
    "            print('New calc - 6 total')\n",
    "            #badcube = iris.load_cube(r'/Users/earmscha/Documents/Fifth_Year/Sem1+2/MPhys/data/favs/'+str(file))\n",
    "            cube = iris.load_cube(r'/Users/earmscha/Documents/Fifth_Year/Sem1+2/MPhys/data/favs/'+str(file))\n",
    "            sos_profile = []\n",
    "            for i in range(75):\n",
    "                cube_proj = change_proj(r'/Users/earmscha/Documents/Fifth_Year/Sem1+2/MPhys/data/weird_cube.nc', cube, i)\n",
    "                area_of_interest = find_area2d(cube_proj, [-170, 30, -130, 40])\n",
    "                \n",
    "                sos = create_2dsos_profile(area_of_interest).data\n",
    "                sos_profile.append(sos)\n",
    "            sos_list.append(sos_profile)\n",
    "        mean_sos = np.mean(sos_list, axis=0)\n",
    "        var_sos = np.std(sos_list, axis=0)\n",
    "        depth = cube.coord('depth').points\n",
    "    else:\n",
    "        for file in file_list:\n",
    "            cube = iris.load_cube(r'/Users/earmscha/Documents/Fifth_Year/Sem1+2/MPhys/data/favs/'+str(file))\n",
    "            area_of_interest = find_area(cube)\n",
    "            sos_profile = create_sos_profile(area_of_interest).data\n",
    "            sos_list.append(sos_profile)\n",
    "        mean_sos = np.mean(sos_list, axis=0)\n",
    "        var_sos = np.std(sos_list, axis=0)\n",
    "        depth = cube.coord('depth').points\n",
    "    return mean_sos, var_sos, depth\n",
    "\n",
    "def create_env(sos_profile, ray_num, my_range, freq, my_depth):\n",
    "    my_ssp = []\n",
    "    for i in range(len(depth_shal)):\n",
    "        point = [depth_shal[i], sos_profile[i]]\n",
    "        my_ssp.append(point)\n",
    "\n",
    "    my_ssp.insert(0,[0,sos_profile[0]])\n",
    "    #bathy = [\n",
    "    #    [0, depth_shal[-1]],    # 30 m water depth at the transmitter\n",
    "    #    [my_range, depth_shal[-1]] # 25 m water depth at 1 km\n",
    "    #]\n",
    "    bathy = [\n",
    "        [0, depth_shal[-1]],    # 30 m water depth at the transmitter\n",
    "        [my_range, depth_shal[-1]] # 25 m water depth at 1 km\n",
    "    ]\n",
    "\n",
    "    my_env = pm.create_env2d(\n",
    "        depth=bathy,\n",
    "        soundspeed=my_ssp,\n",
    "        bottom_soundspeed=1450,\n",
    "        bottom_density=1700,\n",
    "        bottom_absorption=1.0,\n",
    "        tx_depth=depth_shal[my_depth],\n",
    "        frequency=freq,\n",
    "        rx_range = my_range,\n",
    "        rx_depth = depth_shal[my_depth],\n",
    "        nbeams = ray_num,\n",
    "        min_angle = -80,\n",
    "        max_angle = 80,\n",
    "    )\n",
    "    return my_env\n",
    "\n",
    "def my_ray_calc(my_env):\n",
    "    rays = pm.compute_rays(my_env)\n",
    "    return rays\n",
    "\n",
    "def my_ray_plot(profile, rays):\n",
    "    ray_num = float(input('Choose number of rays to calculate:'))\n",
    "    my_range = 1000*float(input('What range to cover (km):'))\n",
    "    freq = float(input('Choose frequency:'))\n",
    "    env = create_env(profile, ray_num, my_range, freq)\n",
    "    rays = my_ray_calc(env)\n",
    "    pm.plot_rays(env, rays)\n",
    "    \n",
    "def my_transmission_calc(my_env, mode):\n",
    "    tloss = pm.compute_transmission_loss(my_env, mode)\n",
    "    return tloss\n",
    "\n",
    "def my_transmission_plot(my_tloss, my_env):\n",
    "    pm.plot_transmission_loss(my_tloss, env=my_env, clim=[-100,0], width=900)\n",
    "    \n",
    "def real_tl_calc(tloss):\n",
    "    real_tl = 20*np.log10(fi.epsilon+np.abs(np.flipud(np.array(tloss))))\n",
    "    return real_tl\n",
    "\n",
    "def demote_dims(cube_1d, cube_2d):\n",
    "\n",
    "    #find the 2d coords of lat\n",
    "    coord2d_lat = cube_2d.coord('latitude')\n",
    "    coord2d_lon = cube_2d.coord('longitude')\n",
    "    #create 1d coords\n",
    "    newcoord2 = iris.coords.DimCoord.from_coord(cube_1d.coord('latitude'))\n",
    "    newcoord1 = iris.coords.DimCoord.from_coord(cube_1d.coord('longitude'))\n",
    "\n",
    "    #remove the lat from 1d coords\n",
    "    cube_1d.remove_coord('latitude')\n",
    "    cube_1d.remove_coord('longitude')\n",
    "\n",
    "    #add 2dcoord as aux\n",
    "    cube_1d.add_aux_coord(coord2d_lat, (0,1))\n",
    "    cube_1d.add_aux_coord(coord2d_lon, (0,1))\n",
    "\n",
    "    #The 1d coordinates have now been updated into 2d\n",
    "    return cube_1d\n",
    "\n",
    "def change_proj(weird_path, cube, index):\n",
    "    target_proj = ccrs.PlateCarree()\n",
    "    weird_cube = iris.load_cube(str(weird_path))[index]\n",
    "    rootgrp = nc4.Dataset(str(weird_path))\n",
    "    cube_1d = cube[index]\n",
    "    cube_2d = demote_dims(cube_1d, weird_cube)\n",
    "    new_cube, extent1 = iris.analysis.cartography.project(cube_2d, target_proj, nx=360, ny=330)\n",
    "    return new_cube\n",
    "\n",
    "def find_area2d(cube, bbox):\n",
    "    \"\"\"\n",
    "    Extract a sub-set of a cube inside a lon, lat bounding box\n",
    "    bbox=[lon_min lon_max lat_min lat_max].\n",
    "    NOTE: This is a work around too subset an iris cube that has\n",
    "    2D lon, lat coords.\n",
    "    \n",
    "    \"\"\"\n",
    "    lons = cube.coord('longitude').points\n",
    "    lats = cube.coord('latitude').points\n",
    "    lons = wrap_lon180(lons)\n",
    "\n",
    "    inregion = np.logical_and(np.logical_and(lons > bbox[0],\n",
    "                                             lons < bbox[2]),\n",
    "                              np.logical_and(lats > bbox[1],\n",
    "                                             lats < bbox[3]))\n",
    "    region_inds = np.where(inregion)\n",
    "    imin, imax = minmax(region_inds[0])\n",
    "    jmin, jmax = minmax(region_inds[1])\n",
    "    return cube[..., imin:imax+1, jmin:jmax+1]"
   ]
